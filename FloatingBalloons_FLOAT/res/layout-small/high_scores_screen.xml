<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/blank_screen" >

    <ImageView
        android:id="@+id/title"
        android:layout_width="200dp"
        android:layout_height="75dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="18dp"
        android:scaleType="fitXY"
        android:src="@drawable/high_score_title" />

    <ImageView
        android:id="@+id/four_num"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_alignLeft="@+id/three_num"
        android:layout_below="@+id/three_num"
        android:scaleType="fitXY"
        android:src="@drawable/four" />

    <TextView
        android:id="@+id/one"
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:layout_alignLeft="@+id/title"
        android:layout_alignTop="@+id/one_num"
        android:layout_marginLeft="32dp"
        android:layout_toRightOf="@+id/one_num"
        android:textColor="#000000"
        android:text="TextView"
        android:textSize="30dp" />

    <TextView
        android:id="@+id/two"
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:layout_alignLeft="@+id/one"
        android:textColor="#000000"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@+id/two_num"
        android:text="TextView"
        android:textSize="30dp" />

    <TextView
        android:id="@+id/three"
        android:layout_width="wrap_content"
        android:textColor="#000000"
        android:layout_height="35dp"
        android:layout_above="@+id/four"
        android:layout_alignLeft="@+id/two"
        android:layout_toRightOf="@+id/three_num"
        android:text="TextView"
        android:textSize="30dp" />

    <TextView
        android:id="@+id/four"
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:layout_alignLeft="@+id/three"
        android:layout_alignTop="@+id/four_num"
        android:layout_toRightOf="@+id/four_num"
        android:textColor="#000000"
        android:text="TextView"
        android:textSize="30dp" />

    <TextView
        android:id="@+id/five"
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:layout_alignLeft="@+id/four"
        android:textColor="#000000"
        android:layout_alignTop="@+id/five_num"
        android:layout_toRightOf="@+id/five_num"
        android:text="TextView"
        android:textSize="30dp" />

    <ImageView
        android:id="@+id/one_num"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_above="@+id/two_num"
        android:layout_toLeftOf="@+id/title"
        android:scaleType="fitXY"
        android:src="@drawable/one" />

    <ImageView
        android:id="@+id/two_num"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_above="@+id/three_num"
        android:layout_toLeftOf="@+id/one"
        android:scaleType="fitXY"
        android:src="@drawable/two" />

    <ImageView
        android:id="@+id/three_num"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_alignLeft="@+id/two_num"
        android:layout_below="@+id/two"
        android:scaleType="fitXY"
        android:src="@drawable/three" />

    <ImageView
        android:id="@+id/five_num"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_alignLeft="@+id/four_num"
        android:layout_below="@+id/four_num"
        android:scaleType="fitXY"
        android:src="@drawable/five" />

    <ImageView
        android:id="@+id/line"
        android:layout_width="275dp"
        android:layout_height="1dp"
        android:layout_below="@+id/title"
        android:layout_centerHorizontal="true"
        android:scaleType="fitXY"
        android:src="@drawable/line" />

</RelativeLayout>