<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:scaleType="fitXY"
        android:src="@drawable/blank_screen" />

    <ImageView
        android:id="@+id/title"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="18dp"
        android:scaleType="fitXY"
        android:src="@drawable/high_score_title" />

    <ImageView
        android:layout_width="250dp"
        android:layout_height="1dp"
        android:layout_alignLeft="@+id/title"
        android:layout_below="@+id/title"
        android:scaleType="fitXY"
        android:src="@drawable/line" />

    <ImageView
        android:id="@+id/one_num"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/title"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="40dp"
        android:scaleType="fitXY"
        android:src="@drawable/one" />
    
    <ImageView
        android:id="@+id/two_num"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/one_num"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="40dp"
        android:scaleType="fitXY"
        android:src="@drawable/two" />
    
    <ImageView
        android:id="@+id/three_num"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/two_num"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="40dp"
        android:scaleType="fitXY"
        android:src="@drawable/three" />
    
    <ImageView
        android:id="@+id/four_num"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/three_num"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="40dp"
        android:scaleType="fitXY"
        android:src="@drawable/four" />
    
    <ImageView
        android:id="@+id/five_num"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/four_num"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="40dp"
        android:scaleType="fitXY"
        android:src="@drawable/five" />

    <TextView
        android:id="@+id/one"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:textColor="#000000"
        android:layout_above="@+id/two_num"
        android:layout_toRightOf="@+id/one_num"
        android:text="TextView"
        android:textSize="40dp" />

    <TextView
        android:id="@+id/two"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:textColor="#000000"
        android:layout_above="@+id/three_num"
        android:layout_alignLeft="@+id/one"
        android:text="TextView"
        android:textSize="40dp" />

    <TextView
        android:id="@+id/three"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:layout_above="@+id/four_num"
        android:layout_toRightOf="@+id/three_num"
        android:textColor="#000000"
        android:text="TextView"
        android:textSize="40dp" />

    <TextView
        android:id="@+id/four"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:layout_above="@+id/five_num"
        android:textColor="#000000"
        android:layout_toRightOf="@+id/four_num"
        android:text="TextView"
        android:textSize="40dp" />

    <TextView
        android:id="@+id/five"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:layout_alignBottom="@+id/five_num"
        android:textColor="#000000"
        android:layout_toRightOf="@+id/five_num"
        android:text="TextView"
        android:textSize="40dp" />

</RelativeLayout>